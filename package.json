{
  "name": "mariposa",
  "version": "1.0.0",
  "description": "a graphql migration and visualization tool",
  "main": "./dist/main.js",
  "scripts": {
    "test": "jest",
    "dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron ./dist/main.js",
    "dev:react": "NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "build": "NODE_ENV=production webpack --config ./webpack.react.config.js && NODE_ENV=production webpack --config ./webpack.electron.config.js",
    "prod:server": "NODE_ENV=production ts-node server/server.ts",
    "dev:server": "NODE_ENV=development nodemon server/server.ts",
    "start": "NODE_ENV=production npm run build && electron ./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/mariposa.git"
  },
  "author": "Adam Berri, Alex Barbazan, James Maguire, Mark Dolan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oslabs-beta/mariposa/issues"
  },
  "homepage": "https://github.com/oslabs-beta/mariposa#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "electron": "^16.0.1",
    "html-webpack-plugin": "^5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-electron-reload": "^1.0.1"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "babel-loader": "^8.2.3",
    "electron-reload": "^2.0.0-alpha.1",
    "express": "^4.17.1"
  }
}
